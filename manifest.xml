<package>
    <description brief="Localization Kalman Filter">
        Kalman Filter for localization by fusing odometry, GPS, and compass.
    </description>
    <author>Michael Koval</author>
    <license>BSD</license>
    <url>https://github.com/mkoval/robot_kf</url>
    <depend package="eigen"/>
    <depend package="geometry_msgs"/>
    <depend package="nav_msgs"/>
    <depend package="roscpp"/>
    <export>
        <cpp cflags="-I${prefix}/include" lflags="-L${prefix}/lib -lkalman_kf"/>
    </export>
</package>
